		"@typescript-eslint/eslint-plugin": "6.8.0",
		"@typescript-eslint/parser": "6.8.0",
		"ajv": "8.12.0",
		"cypress": "13.3.0",
		"eslint": "8.51.0",
		"prettier": "3.0.3",
		"typescript": "5.2.2"
	},
	"cypress-cucumber-preprocessor": { 
		"stepDefinitions": [
			"cypress/e2e/[filepath]/**/*.{js,ts}",
